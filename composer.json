{
    "name": "trippo/laravel-openstack",
    "license": "MIT",
    "description": "OpenStack filesystem service provider for Laravel",
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "5.1 - 5.8|^6.0|^7.0",
        "illuminate/filesystem": "5.1 - 5.8|^6.0|^7.0",
        "league/flysystem-rackspace": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Trippo\\": "src/Trippo/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trippo\\Filesystem\\OpenStackServiceProvider"
            ]
        }
    }
}
